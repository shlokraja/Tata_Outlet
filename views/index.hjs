        #temp_table {
            margin-bottom: 0px;
        }

            #temp_table thead .checkbox label {
                font-weight: bold;
            }

        .total_money {
            margin-left: 300px;
        }

   .signal {
    border: 5px solid #333;
    border-radius: 30px;
    height: 30px;
    left: 50%;
    margin: -15px 0 0 -15px;
    opacity: 0;
    position: absolute;
    top: 50%;
    width: 30px;
 z-index:10000;
    animation: pulsate 1s ease-out;
    animation-iteration-count: infinite;
}

.unscanned-msg, .unscanned-msg-title{    
    font-size: 14px;
}

.unscanned-msg-title{
    color: red; 
}
   
@keyframes pulsate {
    0% {
      transform: scale(.1);
      opacity: 0.0;
    }
    50% {
      opacity: 1;
    }
    100% {
      transform: scale(1.2);
      opacity: 0;
    }
}
    </style>
